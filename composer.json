{
  "name": "andersundsehr/sentry_cron_monitor",
  "description": "Creates sentry cron monitor for all tasks",
  "license": [
    "GPL-2.0-or-later"
  ],
  "type": "typo3-cms-extension",
  "require": {
    "andersundsehr/phpstan-git-files": "^10.1.1",
    "typo3/cms-core": "^12.4 || ^13.4"
  },
  "require-dev": {
    "pluswerk/grumphp-config": "^10.1.3",
    "saschaegerer/phpstan-typo3": "^2.1.1",
    "ssch/typo3-rector": "^3.6.2"
  },
  "autoload": {
    "psr-4": {
      "AUS\\SentryCronMonitor\\": "Classes/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "phpro/grumphp": true,
      "phpstan/extension-installer": true,
      "pluswerk/grumphp-config": true,
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "sentry_cron_monitor"
    }
  }
}
